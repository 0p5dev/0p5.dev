    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#b6c6c4",
    "statusBar.background": "#99b0ad",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#7c9a96",
    "statusBarItem.remoteBackground": "#99b0ad",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#99b0ad",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#99b0ad99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#99b0ad"
